{
  "name": "baby-tracker-service",
  "version": "1.0.0",
  "description": "Backend service for BabyBeat application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "db:init": "node scripts/init-db.js",
    "predev": "npm run db:init",
    "test:setup": "node scripts/setup-test-db.js",
    "test:cleanup": "node scripts/cleanup-test-db.js",
    "test": "npm run test:setup && jest && npm run test:cleanup",
    "test:watch": "npm run test:setup && jest --watch && npm run test:cleanup"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "csv-parse": "^5.6.0",
    "jest": "^29.7.0",
    "pg-connection-string": "^2.6.2",
    "prisma": "^5.10.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
